<!DOCTYPE html>
<html lang="en">

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, viewport-fit=cover, shrink-to-fit=no">
    <meta name="description" content="JourChin Ofough Korosh PhoneOrder">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="theme-color" content="#d94c4c">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <!-- The above tags *must* come first in the head, any other head content must come *after* these tags-->
    <!-- Title-->
    <title>پنل جورچین - @ViewData["Title"]</title>
    <!-- Favicon-->
    <link rel="icon" href="~/favicon.png">
    <!-- Apple Touch Icon-->
    <link rel="apple-touch-icon" href="~/favicon.png">
    <!-- Stylesheet-->
    <link rel="stylesheet" href="~/style.css">
    <link href="~/css/loader.css" rel="stylesheet" />
    <!-- Web App Manifest-->
    <link rel="manifest" href="~/manifest.json">
    <!-- Toast-->
    <link href="~/css/toastr.css" rel="stylesheet" />
    <!-- Sweetalert2 -->
    <link href="~/js/sweetalert2/sweetalert2.min.css" rel="stylesheet" />
</head>
<body>
    <!-- Preloader-->
    <div class="preloader" id="preloader">
        <div class="spinner-grow text-danger" role="status">
            <div class="sr-only">بارگذاری...</div>
        </div>
    </div>

    <div id="ajax-loader" class="ajax-loader">
        <img src="~/img/loader.svg" class="loader-img" alt="لودر">
    </div>

    <div class="login-wrapper d-flex align-items-center justify-content-center text-center">
        <!-- Background Shape-->
        <div class="background-shape"></div>
        @RenderBody()
    </div>
    <!-- All JavaScript Files-->
    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/waypoints.min.js"></script>
    <script src="~/js/jquery.easing.min.js"></script>
    <script src="~/js/owl.carousel.min.js"></script>
    <script src="~/js/jquery.counterup.min.js"></script>
    <script src="~/js/jquery.countdown.min.js"></script>
    <script src="~/js/default/jquery.passwordstrength.js"></script>
    <script src="~/js/wow.min.js"></script>
    <script src="~/js/jarallax.min.js"></script>
    <script src="~/js/jarallax-video.min.js"></script>
    <script src="~/js/default/dark-mode-switch.js"></script>
    <script src="~/js/default/active.js"></script>


    <script src="~/js/site.js"></script>
    <script src="~/js/sweetalert2/sweetalert2.min.js"></script>

    <script src="~/js/toaster/toastr.min.js"></script>
    <script src="~/js/toaster/toast-top-left.js"></script>
    @await RenderSectionAsync("Scripts", required: false)

    <script src="~/js/pwa.js"></script>
    
</body>
</html>
